{
  "name": "@woxly/backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "prisma migrate dev --schema=../../prisma/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema=../../prisma/schema.prisma",
    "db:generate": "prisma generate --schema=../../prisma/schema.prisma",
    "db:studio": "prisma studio --schema=../../prisma/schema.prisma",
    "db:seed": "tsx prisma/seed.ts",
    "admin:create": "tsx src/scripts/createAdmin.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "express": "^4.18.2",
    "socket.io": "^4.6.1",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "zod": "^3.22.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "livekit-server-sdk": "^2.0.0"
  },
  "peerDependencies": {
    "@woxly/shared": "*"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcrypt": "^5.0.2",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/cors": "^2.8.17",
    "@types/speakeasy": "^2.0.10",
    "@types/qrcode": "^1.5.5",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}

